	],
	"portfolio": [
		"name === 'portfolio'",
    "visible === 'skills' && winScrollY > winHeight * 2 + 20 && scrollDist <= -7.5",
    "visible === 'contact' && winScrollY < winHeight * 4 - 20 && scrollDist >= 7.5"
	],
	"contact": [
		"name === 'contact'",
    "visible === 'portfolio' && winScrollY > winHeight * 3 + 20 && scrollDist <= -7.5"
