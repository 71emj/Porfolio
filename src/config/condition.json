{
	"home": [
		"name === 'home'",
    "visible === 'about' && winScrollY < winHeight - 50 && scrollDist >= 60"
	],
	"about": [
		"name === 'about'",
    "visible === 'home' && winScrollY > 50 && scrollDist <= -60",
