	],
	"portfolio": [
		"name === 'portfolio'",
    "visible === 'skills' && winScrollY > winHeight * 2 + 50 && scrollDist <= -60",
    "visible === 'contact' && winScrollY < winHeight * 4 - 50 && scrollDist >= 60"
	],
	"contact": [
		"name === 'contact'",
    "visible === 'portfolio' && winScrollY > winHeight * 3 + 50 && scrollDist <= -60"
	]
